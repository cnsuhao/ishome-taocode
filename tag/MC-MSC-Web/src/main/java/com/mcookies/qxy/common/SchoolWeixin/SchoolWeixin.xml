            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='SchoolWeixinDBO'>
        UPDATE school_weixin SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='SchoolWeixinDBO'>
        DELETE FROM school_weixin WHERE update_time = #{updateTime}
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='SchoolWeixinDBO' resultType='SchoolWeixinDBO'>
        SELECT <include refid='tableColumns'/> FROM school_weixin WHERE 1 = 1
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
    </select>

</mapper>
