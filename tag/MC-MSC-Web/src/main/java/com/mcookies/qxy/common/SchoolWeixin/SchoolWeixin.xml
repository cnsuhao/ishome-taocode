<?xml version='1.0' encoding='UTF-8' ?> 
<!DOCTYPE mapper
    PUBLIC '-//mybatis.org//DTD Dao 3.0//EN' 
    'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<!-- 学校微信企业号表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.mcookies.qxy.common.SchoolWeixin.SchoolWeixinDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        sid,app_id,app_secret,token,wx_id,aes_key,partner_id,partner_key,is_use,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='SchoolWeixinDBO'>
        UPDATE school_weixin SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" appId != null and appId !='' "><!-- 应用ID -->
            ,app_id = #{appId}
        </if>
        <if test=" appSecret != null and appSecret !='' "><!-- 应用密钥 -->
            ,app_secret = #{appSecret}
        </if>
        <if test=" token != null and token !='' "><!-- 应用识别Token -->
            ,token = #{token}
        </if>
        <if test=" wxId != null and wxId !='' "><!-- 微信ID -->
            ,wx_id = #{wxId}
        </if>
        <if test=" aesKey != null and aesKey !='' "><!-- 加密key -->
            ,aes_key = #{aesKey}
        </if>
        <if test=" partnerId != null and partnerId !='' "><!-- 支付商户号 -->
            ,partner_id = #{partnerId}
        </if>
        <if test=" partnerKey != null and partnerKey !='' "><!-- 商户支付密钥 -->
            ,partner_key = #{partnerKey}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
            <if test=" appId != null and appId !='' "><!-- 应用ID -->
                AND app_id = #{appId}
            </if>
            <if test=" appSecret != null and appSecret !='' "><!-- 应用密钥 -->
                AND app_secret = #{appSecret}
            </if>
            <if test=" token != null and token !='' "><!-- 应用识别Token -->
                AND token = #{token}
            </if>
            <if test=" wxId != null and wxId !='' "><!-- 微信ID -->
                AND wx_id = #{wxId}
            </if>
            <if test=" aesKey != null and aesKey !='' "><!-- 加密key -->
                AND aes_key = #{aesKey}
            </if>
            <if test=" partnerId != null and partnerId !='' "><!-- 支付商户号 -->
                AND partner_id = #{partnerId}
            </if>
            <if test=" partnerKey != null and partnerKey !='' "><!-- 商户支付密钥 -->
                AND partner_key = #{partnerKey}
            </if>
            <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
                AND is_use = #{isUse}
            </if>
            <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null and creator !='' "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='SchoolWeixinDBO' resultType='SchoolWeixinDBO'>
        SELECT <include refid='tableColumns'/> FROM school_weixin
        WHERE 1 = 1 
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            AND sid = #{sid}
        </if>
        <if test=" appId != null and appId !='' "><!-- 应用ID -->
            AND app_id = #{appId}
        </if>
        <if test=" appSecret != null and appSecret !='' "><!-- 应用密钥 -->
            AND app_secret = #{appSecret}
        </if>
        <if test=" token != null and token !='' "><!-- 应用识别Token -->
            AND token = #{token}
        </if>
        <if test=" wxId != null and wxId !='' "><!-- 微信ID -->
