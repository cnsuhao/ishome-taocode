        <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
            AND update_time = #{updateTime}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            AND updator = #{updator}
        </if>
    </select>

    <!-- 插入一条数据 -->
    <insert id='doInsert' parameterType='SchoolWeixinDBO'>
        INSERT INTO school_weixin
          ( <include refid='tableColumns'/>) 
        VALUES 
          ( #{sid},#{appId},#{appSecret},#{token},#{wxId},#{aesKey},#{partnerId},#{partnerKey},#{isUse},#{createTime},#{creator},#{updateTime},#{updator} )
    </insert>
    <!-- 修改一条数据 -->
    <update id='doUpdate' parameterType='SchoolWeixinDBO'>
        UPDATE school_weixin SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" appId != null and appId !='' "><!-- 应用ID -->
            ,app_id = #{appId}
        </if>
        <if test=" appSecret != null and appSecret !='' "><!-- 应用密钥 -->
