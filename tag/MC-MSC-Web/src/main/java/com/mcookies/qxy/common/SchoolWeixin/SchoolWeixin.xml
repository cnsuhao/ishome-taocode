        </if>
        <if test=" aesKey != null and aesKey !='' "><!-- 加密key -->
            AND aes_key = #{aesKey}
        </if>
        <if test=" partnerId != null and partnerId !='' "><!-- 支付商户号 -->
            AND partner_id = #{partnerId}
        </if>
        <if test=" partnerKey != null and partnerKey !='' "><!-- 商户支付密钥 -->
