<?xml version='1.0' encoding='UTF-8' ?> 
<!DOCTYPE mapper
    PUBLIC '-//mybatis.org//DTD Dao 3.0//EN' 
    'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<!-- 设备分组表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='DeviceGroup.DeviceGroupDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        dgroup_id,sid,dgroup_name,dgroup_explain,is_use,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='DeviceGroupDBO'>
        UPDATE device_group SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" dgroupId != null and dgroupId !='' "><!-- 分组id -->
            ,dgroup_id = #{dgroupId}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" dgroupName != null and dgroupName !='' "><!-- 分组名称 -->
            ,dgroup_name = #{dgroupName}
        </if>
        <if test=" dgroupExplain != null and dgroupExplain !='' "><!-- 分组说明 -->
            ,dgroup_explain = #{dgroupExplain}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" dgroupId != null and dgroupId !='' "><!-- 分组id -->
                AND dgroup_id = #{dgroupId}
            </if>
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
            <if test=" dgroupName != null and dgroupName !='' "><!-- 分组名称 -->
                AND dgroup_name = #{dgroupName}
            </if>
            <if test=" dgroupExplain != null and dgroupExplain !='' "><!-- 分组说明 -->
                AND dgroup_explain = #{dgroupExplain}
            </if>
            <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
                AND is_use = #{isUse}
            </if>
            <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null and creator !='' "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='DeviceGroupDBO' resultType='DeviceGroupDBO'>
        SELECT <include refid='tableColumns'/> FROM device_group
        WHERE 1 = 1 
        <if test=" dgroupId != null and dgroupId !='' "><!-- 分组id -->
            AND dgroup_id = #{dgroupId}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            AND sid = #{sid}
        </if>
        <if test=" dgroupName != null and dgroupName !='' "><!-- 分组名称 -->
            AND dgroup_name = #{dgroupName}
        </if>
        <if test=" dgroupExplain != null and dgroupExplain !='' "><!-- 分组说明 -->
            AND dgroup_explain = #{dgroupExplain}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            AND is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            AND create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            AND creator = #{creator}
        </if>
        <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
            AND update_time = #{updateTime}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            AND updator = #{updator}
        </if>
    </select>

    <!-- 插入一条数据 -->
    <insert id='doInsert' parameterType='DeviceGroupDBO'>
        INSERT INTO device_group
          ( <include refid='tableColumns'/>) 
        VALUES 
          ( #{dgroupId},#{sid},#{dgroupName},#{dgroupExplain},#{isUse},#{createTime},#{creator},#{updateTime},#{updator} )
    </insert>
    <!-- 修改一条数据 -->
    <update id='doUpdate' parameterType='DeviceGroupDBO'>
        UPDATE device_group SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" dgroupId != null and dgroupId !='' "><!-- 分组id -->
            ,dgroup_id = #{dgroupId}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" dgroupName != null and dgroupName !='' "><!-- 分组名称 -->
            ,dgroup_name = #{dgroupName}
        </if>
        <if test=" dgroupExplain != null and dgroupExplain !='' "><!-- 分组说明 -->
            ,dgroup_explain = #{dgroupExplain}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" dgroupId != null and dgroupId !='' "><!-- 分组id -->
                AND dgroup_id = #{dgroupId}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='DeviceGroupDBO'>
        UPDATE device_group SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" dgroupId != null and dgroupId !='' "><!-- 分组id -->
                AND dgroup_id = #{dgroupId}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='DeviceGroupDBO'>
        DELETE FROM device_group WHERE update_time = #{updateTime}
            <if test=" dgroupId != null and dgroupId !='' "><!-- 分组id -->
                AND dgroup_id = #{dgroupId}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='DeviceGroupDBO' resultType='DeviceGroupDBO'>
        SELECT <include refid='tableColumns'/> FROM device_group WHERE 1 = 1
            <if test=" dgroupId != null and dgroupId !='' "><!-- 分组id -->
                AND dgroup_id = #{dgroupId}
            </if>
    </select>

</mapper>
