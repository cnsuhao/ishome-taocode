        <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
            AND update_time = #{updateTime}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            AND updator = #{updator}
        </if>
    </select>

    <!-- 插入一条数据 -->
    <insert id='doInsert' parameterType='MecItemDBO'>
        INSERT INTO mec_item
          ( <include refid='tableColumns'/>) 
        VALUES 
          ( #{mecItemId},#{sid},#{mecItemName},#{mecItemExplain},#{ruleNum},#{initialScore},#{intervalScore},#{auth},#{tids},#{isUse},#{createTime},#{creator},#{updateTime},#{updator} )
