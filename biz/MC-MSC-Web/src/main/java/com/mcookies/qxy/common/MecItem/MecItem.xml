        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" mecItemId != null and mecItemId !='' "><!-- 德育班级项目id -->
                AND mec_item_id = #{mecItemId}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='MecItemDBO'>
        UPDATE mec_item SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" mecItemId != null and mecItemId !='' "><!-- 德育班级项目id -->
                AND mec_item_id = #{mecItemId}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='MecItemDBO'>
        DELETE FROM mec_item WHERE update_time = #{updateTime}
            <if test=" mecItemId != null and mecItemId !='' "><!-- 德育班级项目id -->
                AND mec_item_id = #{mecItemId}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='MecItemDBO' resultType='MecItemDBO'>
        SELECT <include refid='tableColumns'/> FROM mec_item WHERE 1 = 1
            <if test=" mecItemId != null and mecItemId !='' "><!-- 德育班级项目id -->
