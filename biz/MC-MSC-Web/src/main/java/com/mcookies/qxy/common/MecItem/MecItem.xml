<!-- 德育班级项目表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.mcookies.qxy.common.MecItem.MecItemDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        mec_item_id,sid,mec_item_name,mec_item_explain,rule_num,initial_score,interval_score,auth,tids,is_use,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='MecItemDBO'>
        UPDATE mec_item SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" mecItemId != null and mecItemId !='' "><!-- 德育班级项目id -->
            ,mec_item_id = #{mecItemId}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" mecItemName != null and mecItemName !='' "><!-- 德育班级项目名称 -->
            ,mec_item_name = #{mecItemName}
        </if>
        <if test=" mecItemExplain != null and mecItemExplain !='' "><!-- 德育班级项目说明 -->
            ,mec_item_explain = #{mecItemExplain}
        </if>
        <if test=" ruleNum != null and ruleNum !='' "><!-- 规则分数量 -->
            ,rule_num = #{ruleNum}
        </if>
        <if test=" initialScore != null and initialScore !='' "><!-- 初始分 -->
            ,initial_score = #{initialScore}
        </if>
        <if test=" intervalScore != null and intervalScore !='' "><!-- 间隔分 -->
            ,interval_score = #{intervalScore}
        </if>
        <if test=" auth != null and auth !='' "><!-- 权限 -->
            ,auth = #{auth}
        </if>
        <if test=" tids != null and tids !='' "><!-- 可见教师集合 -->
            ,tids = #{tids}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" mecItemId != null and mecItemId !='' "><!-- 德育班级项目id -->
                AND mec_item_id = #{mecItemId}
            </if>
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
            <if test=" mecItemName != null and mecItemName !='' "><!-- 德育班级项目名称 -->
                AND mec_item_name = #{mecItemName}
            </if>
            <if test=" mecItemExplain != null and mecItemExplain !='' "><!-- 德育班级项目说明 -->
                AND mec_item_explain = #{mecItemExplain}
            </if>
            <if test=" ruleNum != null and ruleNum !='' "><!-- 规则分数量 -->
                AND rule_num = #{ruleNum}
            </if>
            <if test=" initialScore != null and initialScore !='' "><!-- 初始分 -->
                AND initial_score = #{initialScore}
            </if>
            <if test=" intervalScore != null and intervalScore !='' "><!-- 间隔分 -->
                AND interval_score = #{intervalScore}
            </if>
            <if test=" auth != null and auth !='' "><!-- 权限 -->
                AND auth = #{auth}
            </if>
            <if test=" tids != null and tids !='' "><!-- 可见教师集合 -->
                AND tids = #{tids}
            </if>
            <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
                AND is_use = #{isUse}
            </if>
            <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null and creator !='' "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='MecItemDBO' resultType='MecItemDBO'>
        SELECT <include refid='tableColumns'/> FROM mec_item
        WHERE 1 = 1 
        <if test=" mecItemId != null and mecItemId !='' "><!-- 德育班级项目id -->
            AND mec_item_id = #{mecItemId}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
