			AND updator = #{updator}
		</if>
	</select>

	<!-- 插入一条数据 -->
	<insert id='doInsert' parameterType='OaRuleDBO'>
		INSERT INTO oa_rule
		(
		<include refid='tableColumns' />
		)
		VALUES
		(
		#{oaruleId},#{sid},#{oatagsId},#{oaruleName},#{serialNumber},#{adoptType},#{tids},#{isUse},#{createTime},#{creator},#{updateTime},#{updator}
		)
