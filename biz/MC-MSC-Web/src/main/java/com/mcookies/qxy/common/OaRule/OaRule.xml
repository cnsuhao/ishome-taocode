		</if>
	</update>
	<!-- 查询分页数据信息 -->
	<select id='doSelectPage' parameterType='OaRuleDBO' resultType='OaRuleDBO'>
		SELECT
		<include refid='tableColumns' />
		FROM oa_rule
		WHERE 1 = 1
		<if test=" oaruleId != null and oaruleId !='' "><!-- 规则id -->
			AND oarule_id = #{oaruleId}
		</if>
		<if test=" sid != null and sid !='' "><!-- 学校id -->
