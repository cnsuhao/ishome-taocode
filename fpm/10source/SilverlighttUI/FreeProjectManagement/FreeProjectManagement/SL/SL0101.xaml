<UserControl x:Class="FreeProjectManagement.SL.SL0101"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="340">    
    <Grid x:Name="LayoutRoot" Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="280" />
            <ColumnDefinition Width="60*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="180" />
            <RowDefinition Height="50" />
            <RowDefinition Height="50" />
            <RowDefinition Height="20*" />
        </Grid.RowDefinitions>
        <TextBox Name="textBox1" Grid.Row="1" Margin="5" />
        <TextBox Name="textBox2" Grid.Row="2" Margin="5" />
        <Button Content="Button" Grid.Row="2" Name="button1" Click="button1_Click" Grid.Column="2" Margin="5" />
    </Grid>
</UserControl>
