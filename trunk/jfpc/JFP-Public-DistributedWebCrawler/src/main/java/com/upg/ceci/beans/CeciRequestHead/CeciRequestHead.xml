<?xml version='1.0' encoding='UTF-8' ?> 
<!DOCTYPE mapper
    PUBLIC '-//mybatis.org//DTD Dao 3.0//EN' 
    'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<!-- 抓取地址请求头配置 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.upg.ceci.beans.CeciRequestHead.CeciRequestHeadDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        id,area_code,head_name,val,request_info_id,type
    </sql>

    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='CeciRequestHeadDBO' resultType='CeciRequestHeadDBO'>
        SELECT <include refid='tableColumns'/> FROM ceci_request_head
        WHERE 1 = 1 
        <if test=" id != null and id !='' "><!-- 主键ID -->
            AND id = #{id}
        </if>
       
        <if test=" requestInfoId != null and requestInfoId !='' "><!-- 请求信息id -->
            AND request_info_id = #{requestInfoId}
        </if>
        
		<if test=" areaCode != null and areaCode !='' "><!-- 区域代码 -->
            AND area_code = #{areaCode}
        </if>
    </select>

</mapper>
