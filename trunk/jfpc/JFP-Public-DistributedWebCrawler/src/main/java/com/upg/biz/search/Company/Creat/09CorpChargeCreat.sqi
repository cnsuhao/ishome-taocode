<?xml version='1.0' encoding='UTF-8'?>
<sentences>
	<creat>
		<id>corp_charge_creat</id>
		<index>corp_charge_list</index>
		<!-- 判决信息 -->
		<sql>
			<![CDATA[
			
			SELECT 
				 charge.t_date,
				 charge.charge_id,						#判决书编号
				 charge.charge,							#判决书内容
				 
			FROM corp_charge charge 
							
			WHERE
				     charge.id is not null  
				
				ORDER BY charge.charge_id 
				
				LIMIT 1
				
			]]>
		</sql>
	</creat>
</sentences>