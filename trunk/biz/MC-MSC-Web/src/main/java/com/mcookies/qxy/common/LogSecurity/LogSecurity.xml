        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='LogSecurityDBO'>
        UPDATE log_security SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='LogSecurityDBO'>
        DELETE FROM log_security WHERE update_time = #{updateTime}
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='LogSecurityDBO' resultType='LogSecurityDBO'>
        SELECT <include refid='tableColumns'/> FROM log_security WHERE 1 = 1
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </select>

</mapper>
