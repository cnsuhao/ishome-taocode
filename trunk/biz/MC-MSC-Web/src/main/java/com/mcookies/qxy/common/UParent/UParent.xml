        </if>
        <if test=" gender != null and gender !='' "><!-- 性别 -->
            ,gender = #{gender}
        </if>
        <if test=" status != null and status !='' "><!-- 状态 -->
            ,status = #{status}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" parentId != null and parentId !='' "><!-- 家长id（自增） -->
                AND parent_id = #{parentId}
            </if>
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
            <if test=" uid != null and uid !='' "><!-- 用户id -->
                AND uid = #{uid}
            </if>
            <if test=" parentName != null and parentName !='' "><!-- 家长姓名 -->
                AND parent_name = #{parentName}
            </if>
            <if test=" phone != null and phone !='' "><!-- 家长电话 -->
                AND phone = #{phone}
            </if>
            <if test=" workUnit != null and workUnit !='' "><!-- 工作单位 -->
                AND work_unit = #{workUnit}
            </if>
            <if test=" position != null and position !='' "><!-- 职位 -->
                AND position = #{position}
            </if>
            <if test=" method != null and method !='' "><!-- 认证方式 -->
                AND method = #{method}
            </if>
            <if test=" openid != null and openid !='' "><!-- openid -->
                AND openid = #{openid}
            </if>
            <if test=" wechatname != null and wechatname !='' "><!-- 微信名 -->
