    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='UParentDBO'>
        UPDATE u_parent SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" parentId != null and parentId !='' "><!-- 家长id（自增） -->
                AND parent_id = #{parentId}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='UParentDBO'>
        DELETE FROM u_parent WHERE update_time = #{updateTime}
            <if test=" parentId != null and parentId !='' "><!-- 家长id（自增） -->
                AND parent_id = #{parentId}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='UParentDBO' resultType='UParentDBO'>
