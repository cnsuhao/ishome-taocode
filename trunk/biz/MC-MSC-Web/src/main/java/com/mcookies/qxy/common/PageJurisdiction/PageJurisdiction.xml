<?xml version='1.0' encoding='UTF-8' ?> 
<!DOCTYPE mapper
    PUBLIC '-//mybatis.org//DTD Dao 3.0//EN' 
    'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<!-- 页面权限管理表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.mcookies.qxy.common.PageJurisdiction.PageJurisdictionDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        jurisdiction_id,content,page_id,identity,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='PageJurisdictionDBO'>
        UPDATE page_jurisdiction SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" jurisdictionId != null and jurisdictionId !='' "><!-- 页面权限id -->
            ,jurisdiction_id = #{jurisdictionId}
        </if>
        <if test=" content != null and content !='' "><!-- 权限内容 -->
            ,content = #{content}
        </if>
        <if test=" pageId != null and pageId !='' "><!-- 对应页面 -->
            ,page_id = #{pageId}
        </if>
        <if test=" identity != null and identity !='' "><!-- 对应身份 -->
            ,identity = #{identity}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" jurisdictionId != null and jurisdictionId !='' "><!-- 页面权限id -->
                AND jurisdiction_id = #{jurisdictionId}
            </if>
            <if test=" content != null and content !='' "><!-- 权限内容 -->
                AND content = #{content}
            </if>
            <if test=" pageId != null and pageId !='' "><!-- 对应页面 -->
                AND page_id = #{pageId}
            </if>
            <if test=" identity != null and identity !='' "><!-- 对应身份 -->
                AND identity = #{identity}
            </if>
            <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null and creator !='' "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='PageJurisdictionDBO' resultType='PageJurisdictionDBO'>
        SELECT <include refid='tableColumns'/> FROM page_jurisdiction
        WHERE 1 = 1 
        <if test=" jurisdictionId != null and jurisdictionId !='' "><!-- 页面权限id -->
            AND jurisdiction_id = #{jurisdictionId}
        </if>
        <if test=" content != null and content !='' "><!-- 权限内容 -->
            AND content = #{content}
        </if>
        <if test=" pageId != null and pageId !='' "><!-- 对应页面 -->
            AND page_id = #{pageId}
        </if>
        <if test=" identity != null and identity !='' "><!-- 对应身份 -->
            AND identity = #{identity}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            AND create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            AND creator = #{creator}
        </if>
        <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
            AND update_time = #{updateTime}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            AND updator = #{updator}
        </if>
    </select>

    <!-- 插入一条数据 -->
    <insert id='doInsert' parameterType='PageJurisdictionDBO'>
        INSERT INTO page_jurisdiction
          ( <include refid='tableColumns'/>) 
        VALUES 
          ( #{jurisdictionId},#{content},#{pageId},#{identity},#{createTime},#{creator},#{updateTime},#{updator} )
    </insert>
    <!-- 修改一条数据 -->
    <update id='doUpdate' parameterType='PageJurisdictionDBO'>
        UPDATE page_jurisdiction SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" jurisdictionId != null and jurisdictionId !='' "><!-- 页面权限id -->
            ,jurisdiction_id = #{jurisdictionId}
        </if>
        <if test=" content != null and content !='' "><!-- 权限内容 -->
            ,content = #{content}
        </if>
        <if test=" pageId != null and pageId !='' "><!-- 对应页面 -->
            ,page_id = #{pageId}
        </if>
        <if test=" identity != null and identity !='' "><!-- 对应身份 -->
            ,identity = #{identity}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" jurisdictionId != null and jurisdictionId !='' "><!-- 页面权限id -->
                AND jurisdiction_id = #{jurisdictionId}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='PageJurisdictionDBO'>
        UPDATE page_jurisdiction SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" jurisdictionId != null and jurisdictionId !='' "><!-- 页面权限id -->
                AND jurisdiction_id = #{jurisdictionId}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='PageJurisdictionDBO'>
        DELETE FROM page_jurisdiction WHERE update_time = #{updateTime}
            <if test=" jurisdictionId != null and jurisdictionId !='' "><!-- 页面权限id -->
                AND jurisdiction_id = #{jurisdictionId}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='PageJurisdictionDBO' resultType='PageJurisdictionDBO'>
        SELECT <include refid='tableColumns'/> FROM page_jurisdiction WHERE 1 = 1
            <if test=" jurisdictionId != null and jurisdictionId !='' "><!-- 页面权限id -->
                AND jurisdiction_id = #{jurisdictionId}
            </if>
    </select>

</mapper>
