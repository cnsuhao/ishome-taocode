<?xml version='1.0' encoding='UTF-8' ?> 
<!DOCTYPE mapper
    PUBLIC '-//mybatis.org//DTD Dao 3.0//EN' 
    'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<!-- 教工标签关联表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.mcookies.qxy.common.SLabelTeacher.SLabelTeacherDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        id,sid,label_id,tid,is_use,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='SLabelTeacherDBO'>
        UPDATE s_label_teacher SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" id != null "><!-- 自增id -->
            ,id = #{id}
        </if>
        <if test=" sid != null "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" labelId != null "><!-- 群组标签id -->
            ,label_id = #{labelId}
        </if>
        <if test=" tid != null "><!-- 教师id -->
            ,tid = #{tid}
        </if>
        <if test=" isUse != null "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" id != null "><!-- 自增id -->
                AND id = #{id}
            </if>
            <if test=" sid != null "><!-- 学校id -->
                AND sid = #{sid}
            </if>
            <if test=" labelId != null "><!-- 群组标签id -->
                AND label_id = #{labelId}
            </if>
            <if test=" tid != null "><!-- 教师id -->
                AND tid = #{tid}
            </if>
            <if test=" isUse != null "><!-- 是否启用 -->
                AND is_use = #{isUse}
            </if>
            <if test=" createTime != null "><!-- 创建时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='SLabelTeacherDBO' resultType='SLabelTeacherDBO'>
        SELECT <include refid='tableColumns'/> FROM s_label_teacher
        WHERE 1 = 1 
        <if test=" id != null "><!-- 自增id -->
            AND id = #{id}
        </if>
        <if test=" sid != null "><!-- 学校id -->
            AND sid = #{sid}
        </if>
        <if test=" labelId != null "><!-- 群组标签id -->
            AND label_id = #{labelId}
        </if>
        <if test=" tid != null "><!-- 教师id -->
            AND tid = #{tid}
        </if>
        <if test=" isUse != null "><!-- 是否启用 -->
            AND is_use = #{isUse}
        </if>
        <if test=" createTime != null "><!-- 创建时间 -->
            AND create_time = #{createTime}
        </if>
        <if test=" creator != null "><!-- 创建者 -->
            AND creator = #{creator}
        </if>
        <if test=" updateTime != null "><!-- 更新时间 -->
            AND update_time = #{updateTime}
        </if>
        <if test=" updator != null "><!-- 最后更新者 -->
            AND updator = #{updator}
        </if>
    </select>

    <!-- 插入一条数据 -->
    <insert id='doInsert' parameterType='SLabelTeacherDBO'>
        INSERT INTO s_label_teacher
          ( <include refid='tableColumns'/>) 
        VALUES 
          ( #{id},#{sid},#{labelId},#{tid},#{isUse},#{createTime},#{creator},#{updateTime},#{updator} )
    </insert>
    <!-- 修改一条数据 -->
    <update id='doUpdate' parameterType='SLabelTeacherDBO'>
        UPDATE s_label_teacher SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" id != null "><!-- 自增id -->
            ,id = #{id}
        </if>
        <if test=" sid != null "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" labelId != null "><!-- 群组标签id -->
            ,label_id = #{labelId}
        </if>
        <if test=" tid != null "><!-- 教师id -->
            ,tid = #{tid}
        </if>
        <if test=" isUse != null "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" id != null "><!-- 自增id -->
                AND id = #{id}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='SLabelTeacherDBO'>
        UPDATE s_label_teacher SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" id != null "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='SLabelTeacherDBO'>
        DELETE FROM s_label_teacher WHERE update_time = #{updateTime}
            <if test=" id != null "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='SLabelTeacherDBO' resultType='SLabelTeacherDBO'>
        SELECT <include refid='tableColumns'/> FROM s_label_teacher WHERE 1 = 1
            <if test=" id != null "><!-- 自增id -->
                AND id = #{id}
            </if>
    </select>

</mapper>
