<?xml version='1.0' encoding='UTF-8' ?> 
<!DOCTYPE mapper
    PUBLIC '-//mybatis.org//DTD Dao 3.0//EN' 
    'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<!-- 短信消息记录表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.mcookies.qxy.common.SMessage.SMessageDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        id,type,object,tid,teacher_name,parent_id,parent_name,phone,sending_time,message,isreceipt,status,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='SMessageDBO'>
        UPDATE s_message SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" id != null and id !='' "><!-- 自增id -->
            ,id = #{id}
        </if>
        <if test=" type != null and type !='' "><!-- 消息类型 -->
            ,type = #{type}
        </if>
        <if test=" object != null and object !='' "><!-- 对象类型 -->
            ,object = #{object}
        </if>
        <if test=" tid != null and tid !='' "><!-- 教师id -->
            ,tid = #{tid}
        </if>
        <if test=" teacherName != null and teacherName !='' "><!-- 教师姓名 -->
            ,teacher_name = #{teacherName}
        </if>
        <if test=" parentId != null and parentId !='' "><!-- 家长id -->
            ,parent_id = #{parentId}
        </if>
        <if test=" parentName != null and parentName !='' "><!-- 家长姓名 -->
            ,parent_name = #{parentName}
        </if>
        <if test=" phone != null and phone !='' "><!-- 手机号 -->
            ,phone = #{phone}
        </if>
        <if test=" sendingTime != null and sendingTime !='' "><!-- 发送时间 -->
            ,sending_time = #{sendingTime}
        </if>
        <if test=" message != null and message !='' "><!-- 短信内容 -->
            ,message = #{message}
        </if>
        <if test=" isreceipt != null and isreceipt !='' "><!-- 是否需要回执 -->
            ,isreceipt = #{isreceipt}
        </if>
        <if test=" status != null and status !='' "><!-- 状态 -->
            ,status = #{status}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 登录时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
            <if test=" type != null and type !='' "><!-- 消息类型 -->
                AND type = #{type}
            </if>
            <if test=" object != null and object !='' "><!-- 对象类型 -->
                AND object = #{object}
            </if>
            <if test=" tid != null and tid !='' "><!-- 教师id -->
                AND tid = #{tid}
            </if>
            <if test=" teacherName != null and teacherName !='' "><!-- 教师姓名 -->
                AND teacher_name = #{teacherName}
            </if>
            <if test=" parentId != null and parentId !='' "><!-- 家长id -->
                AND parent_id = #{parentId}
            </if>
            <if test=" parentName != null and parentName !='' "><!-- 家长姓名 -->
                AND parent_name = #{parentName}
            </if>
            <if test=" phone != null and phone !='' "><!-- 手机号 -->
                AND phone = #{phone}
            </if>
            <if test=" sendingTime != null and sendingTime !='' "><!-- 发送时间 -->
                AND sending_time = #{sendingTime}
            </if>
            <if test=" message != null and message !='' "><!-- 短信内容 -->
                AND message = #{message}
            </if>
            <if test=" isreceipt != null and isreceipt !='' "><!-- 是否需要回执 -->
                AND isreceipt = #{isreceipt}
            </if>
            <if test=" status != null and status !='' "><!-- 状态 -->
                AND status = #{status}
            </if>
            <if test=" createTime != null and createTime !='' "><!-- 登录时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null and creator !='' "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='SMessageDBO' resultType='SMessageDBO'>
        SELECT <include refid='tableColumns'/> FROM s_message
        WHERE 1 = 1 
        <if test=" id != null and id !='' "><!-- 自增id -->
            AND id = #{id}
        </if>
        <if test=" type != null and type !='' "><!-- 消息类型 -->
            AND type = #{type}
        </if>
        <if test=" object != null and object !='' "><!-- 对象类型 -->
            AND object = #{object}
        </if>
        <if test=" tid != null and tid !='' "><!-- 教师id -->
            AND tid = #{tid}
        </if>
        <if test=" teacherName != null and teacherName !='' "><!-- 教师姓名 -->
            AND teacher_name = #{teacherName}
        </if>
        <if test=" parentId != null and parentId !='' "><!-- 家长id -->
            AND parent_id = #{parentId}
        </if>
        <if test=" parentName != null and parentName !='' "><!-- 家长姓名 -->
            AND parent_name = #{parentName}
        </if>
        <if test=" phone != null and phone !='' "><!-- 手机号 -->
            AND phone = #{phone}
        </if>
        <if test=" sendingTime != null and sendingTime !='' "><!-- 发送时间 -->
            AND sending_time = #{sendingTime}
        </if>
        <if test=" message != null and message !='' "><!-- 短信内容 -->
            AND message = #{message}
        </if>
        <if test=" isreceipt != null and isreceipt !='' "><!-- 是否需要回执 -->
            AND isreceipt = #{isreceipt}
        </if>
        <if test=" status != null and status !='' "><!-- 状态 -->
            AND status = #{status}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 登录时间 -->
            AND create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            AND creator = #{creator}
        </if>
        <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
            AND update_time = #{updateTime}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            AND updator = #{updator}
        </if>
    </select>

    <!-- 插入一条数据 -->
    <insert id='doInsert' parameterType='SMessageDBO'>
        INSERT INTO s_message
          ( <include refid='tableColumns'/>) 
        VALUES 
          ( #{id},#{type},#{object},#{tid},#{teacherName},#{parentId},#{parentName},#{phone},#{sendingTime},#{message},#{isreceipt},#{status},#{createTime},#{creator},#{updateTime},#{updator} )
    </insert>
    <!-- 修改一条数据 -->
    <update id='doUpdate' parameterType='SMessageDBO'>
        UPDATE s_message SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" id != null and id !='' "><!-- 自增id -->
            ,id = #{id}
        </if>
        <if test=" type != null and type !='' "><!-- 消息类型 -->
            ,type = #{type}
        </if>
        <if test=" object != null and object !='' "><!-- 对象类型 -->
            ,object = #{object}
        </if>
        <if test=" tid != null and tid !='' "><!-- 教师id -->
            ,tid = #{tid}
        </if>
        <if test=" teacherName != null and teacherName !='' "><!-- 教师姓名 -->
            ,teacher_name = #{teacherName}
        </if>
        <if test=" parentId != null and parentId !='' "><!-- 家长id -->
            ,parent_id = #{parentId}
        </if>
        <if test=" parentName != null and parentName !='' "><!-- 家长姓名 -->
            ,parent_name = #{parentName}
        </if>
        <if test=" phone != null and phone !='' "><!-- 手机号 -->
            ,phone = #{phone}
        </if>
        <if test=" sendingTime != null and sendingTime !='' "><!-- 发送时间 -->
            ,sending_time = #{sendingTime}
        </if>
        <if test=" message != null and message !='' "><!-- 短信内容 -->
            ,message = #{message}
        </if>
        <if test=" isreceipt != null and isreceipt !='' "><!-- 是否需要回执 -->
            ,isreceipt = #{isreceipt}
        </if>
        <if test=" status != null and status !='' "><!-- 状态 -->
            ,status = #{status}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 登录时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='SMessageDBO'>
        UPDATE s_message SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='SMessageDBO'>
        DELETE FROM s_message WHERE update_time = #{updateTime}
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='SMessageDBO' resultType='SMessageDBO'>
        SELECT <include refid='tableColumns'/> FROM s_message WHERE 1 = 1
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </select>

</mapper>
