<?xml version='1.0' encoding='UTF-8' ?> 
<!DOCTYPE mapper
    PUBLIC '-//mybatis.org//DTD Dao 3.0//EN' 
    'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<!-- 校历表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.mcookies.qxy.common.SCalendar.SCalendarDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        calendar_id,sid,term_id,start_time,end_time,content,department,is_use,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='SCalendarDBO'>
        UPDATE s_calendar SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" calendarId != null and calendarId !='' "><!-- 自增id -->
            ,calendar_id = #{calendarId}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" termId != null and termId !='' "><!-- 学期id -->
            ,term_id = #{termId}
        </if>
        <if test=" startTime != null and startTime !='' "><!-- 开始时间 -->
            ,start_time = #{startTime}
        </if>
        <if test=" endTime != null and endTime !='' "><!-- 结束时间 -->
            ,end_time = #{endTime}
        </if>
        <if test=" content != null and content !='' "><!-- 校历事项 -->
            ,content = #{content}
        </if>
        <if test=" department != null and department !='' "><!-- 责任部门 -->
            ,department = #{department}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" calendarId != null and calendarId !='' "><!-- 自增id -->
                AND calendar_id = #{calendarId}
            </if>
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
            <if test=" termId != null and termId !='' "><!-- 学期id -->
                AND term_id = #{termId}
            </if>
            <if test=" startTime != null and startTime !='' "><!-- 开始时间 -->
                AND start_time = #{startTime}
            </if>
            <if test=" endTime != null and endTime !='' "><!-- 结束时间 -->
                AND end_time = #{endTime}
            </if>
            <if test=" content != null and content !='' "><!-- 校历事项 -->
                AND content = #{content}
            </if>
            <if test=" department != null and department !='' "><!-- 责任部门 -->
                AND department = #{department}
            </if>
            <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
                AND is_use = #{isUse}
            </if>
            <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null and creator !='' "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='SCalendarDBO' resultType='SCalendarDBO'>
        SELECT <include refid='tableColumns'/> FROM s_calendar
        WHERE 1 = 1 
        <if test=" calendarId != null and calendarId !='' "><!-- 自增id -->
            AND calendar_id = #{calendarId}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            AND sid = #{sid}
        </if>
        <if test=" termId != null and termId !='' "><!-- 学期id -->
            AND term_id = #{termId}
        </if>
        <if test=" startTime != null and startTime !='' "><!-- 开始时间 -->
            AND start_time = #{startTime}
        </if>
        <if test=" endTime != null and endTime !='' "><!-- 结束时间 -->
            AND end_time = #{endTime}
        </if>
        <if test=" content != null and content !='' "><!-- 校历事项 -->
            AND content = #{content}
        </if>
        <if test=" department != null and department !='' "><!-- 责任部门 -->
            AND department = #{department}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            AND is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            AND create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            AND creator = #{creator}
        </if>
        <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
            AND update_time = #{updateTime}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            AND updator = #{updator}
        </if>
    </select>

    <!-- 插入一条数据 -->
    <insert id='doInsert' parameterType='SCalendarDBO'>
        INSERT INTO s_calendar
          ( <include refid='tableColumns'/>) 
        VALUES 
          ( #{calendarId},#{sid},#{termId},#{startTime},#{endTime},#{content},#{department},#{isUse},#{createTime},#{creator},#{updateTime},#{updator} )
    </insert>
    <!-- 修改一条数据 -->
    <update id='doUpdate' parameterType='SCalendarDBO'>
        UPDATE s_calendar SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" calendarId != null and calendarId !='' "><!-- 自增id -->
            ,calendar_id = #{calendarId}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" termId != null and termId !='' "><!-- 学期id -->
            ,term_id = #{termId}
        </if>
        <if test=" startTime != null and startTime !='' "><!-- 开始时间 -->
            ,start_time = #{startTime}
        </if>
        <if test=" endTime != null and endTime !='' "><!-- 结束时间 -->
            ,end_time = #{endTime}
        </if>
        <if test=" content != null and content !='' "><!-- 校历事项 -->
            ,content = #{content}
        </if>
        <if test=" department != null and department !='' "><!-- 责任部门 -->
            ,department = #{department}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" calendarId != null and calendarId !='' "><!-- 自增id -->
                AND calendar_id = #{calendarId}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='SCalendarDBO'>
        UPDATE s_calendar SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" calendarId != null and calendarId !='' "><!-- 自增id -->
                AND calendar_id = #{calendarId}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='SCalendarDBO'>
        DELETE FROM s_calendar WHERE update_time = #{updateTime}
            <if test=" calendarId != null and calendarId !='' "><!-- 自增id -->
                AND calendar_id = #{calendarId}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='SCalendarDBO' resultType='SCalendarDBO'>
        SELECT <include refid='tableColumns'/> FROM s_calendar WHERE 1 = 1
            <if test=" calendarId != null and calendarId !='' "><!-- 自增id -->
                AND calendar_id = #{calendarId}
            </if>
    </select>

</mapper>
