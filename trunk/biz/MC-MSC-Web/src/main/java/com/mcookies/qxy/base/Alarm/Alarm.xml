<?xml version='1.0' encoding='UTF-8' ?> 
<!DOCTYPE mapper
    PUBLIC '-//mybatis.org//DTD Dao 3.0//EN' 
    'http://mybatis.org/dtd/mybatis-3-mapper.dtd' >
<!-- 报警表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.mcookies.qxy.base.Alarm.AlarmDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        id,sid,cid,student_id,date,start_time,end_time,device_id,flag,actual_flag,is_alarm,is_use,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='AlarmDBO'>
        UPDATE alarm SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" id != null and id !='' "><!-- 自增id -->
            ,id = #{id}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" cid != null and cid !='' "><!-- 班级id -->
            ,cid = #{cid}
        </if>
        <if test=" studentId != null and studentId !='' "><!-- 学生id -->
            ,student_id = #{studentId}
        </if>
        <if test=" date != null and date !='' "><!-- 日期 -->
            ,date = #{date}
        </if>
        <if test=" startTime != null and startTime !='' "><!-- 开始时间 -->
            ,start_time = #{startTime}
        </if>
        <if test=" endTime != null and endTime !='' "><!-- 结束时间 -->
            ,end_time = #{endTime}
        </if>
        <if test=" deviceId != null and deviceId !='' "><!-- 学校设备id -->
            ,device_id = #{deviceId}
        </if>
        <if test=" flag != null and flag !='' "><!-- 规定行为状态 -->
            ,flag = #{flag}
        </if>
        <if test=" actualFlag != null and actualFlag !='' "><!-- 实际行为状态 -->
            ,actual_flag = #{actualFlag}
        </if>
        <if test=" isAlarm != null and isAlarm !='' "><!-- 是否报警 -->
            ,is_alarm = #{isAlarm}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
            <if test=" sid != null and sid !='' "><!-- 学校id -->
                AND sid = #{sid}
            </if>
            <if test=" cid != null and cid !='' "><!-- 班级id -->
                AND cid = #{cid}
            </if>
            <if test=" studentId != null and studentId !='' "><!-- 学生id -->
                AND student_id = #{studentId}
            </if>
            <if test=" date != null and date !='' "><!-- 日期 -->
                AND date = #{date}
            </if>
            <if test=" startTime != null and startTime !='' "><!-- 开始时间 -->
                AND start_time = #{startTime}
            </if>
            <if test=" endTime != null and endTime !='' "><!-- 结束时间 -->
                AND end_time = #{endTime}
            </if>
            <if test=" deviceId != null and deviceId !='' "><!-- 学校设备id -->
                AND device_id = #{deviceId}
            </if>
            <if test=" flag != null and flag !='' "><!-- 规定行为状态 -->
                AND flag = #{flag}
            </if>
            <if test=" actualFlag != null and actualFlag !='' "><!-- 实际行为状态 -->
                AND actual_flag = #{actualFlag}
            </if>
            <if test=" isAlarm != null and isAlarm !='' "><!-- 是否报警 -->
                AND is_alarm = #{isAlarm}
            </if>
            <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
                AND is_use = #{isUse}
            </if>
            <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null and creator !='' "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='AlarmDBO' resultType='AlarmDBO'>
        SELECT <include refid='tableColumns'/> FROM alarm
        WHERE 1 = 1 
        <if test=" id != null and id !='' "><!-- 自增id -->
            AND id = #{id}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            AND sid = #{sid}
        </if>
        <if test=" cid != null and cid !='' "><!-- 班级id -->
            AND cid = #{cid}
        </if>
        <if test=" studentId != null and studentId !='' "><!-- 学生id -->
            AND student_id = #{studentId}
        </if>
        <if test=" date != null and date !='' "><!-- 日期 -->
            AND date = #{date}
        </if>
        <if test=" startTime != null and startTime !='' "><!-- 开始时间 -->
            AND start_time = #{startTime}
        </if>
        <if test=" endTime != null and endTime !='' "><!-- 结束时间 -->
            AND end_time = #{endTime}
        </if>
        <if test=" deviceId != null and deviceId !='' "><!-- 学校设备id -->
            AND device_id = #{deviceId}
        </if>
        <if test=" flag != null and flag !='' "><!-- 规定行为状态 -->
            AND flag = #{flag}
        </if>
        <if test=" actualFlag != null and actualFlag !='' "><!-- 实际行为状态 -->
            AND actual_flag = #{actualFlag}
        </if>
        <if test=" isAlarm != null and isAlarm !='' "><!-- 是否报警 -->
            AND is_alarm = #{isAlarm}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            AND is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            AND create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            AND creator = #{creator}
        </if>
        <if test=" updateTime != null and updateTime !='' "><!-- 更新时间 -->
            AND update_time = #{updateTime}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            AND updator = #{updator}
        </if>
    </select>

    <!-- 插入一条数据 -->
    <insert id='doInsert' parameterType='AlarmDBO'>
        INSERT INTO alarm
          ( <include refid='tableColumns'/>) 
        VALUES 
          ( #{id},#{sid},#{cid},#{studentId},#{date},#{startTime},#{endTime},#{deviceId},#{flag},#{actualFlag},#{isAlarm},#{isUse},#{createTime},#{creator},#{updateTime},#{updator} )
    </insert>
    <!-- 修改一条数据 -->
    <update id='doUpdate' parameterType='AlarmDBO'>
        UPDATE alarm SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" id != null and id !='' "><!-- 自增id -->
            ,id = #{id}
        </if>
        <if test=" sid != null and sid !='' "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" cid != null and cid !='' "><!-- 班级id -->
            ,cid = #{cid}
        </if>
        <if test=" studentId != null and studentId !='' "><!-- 学生id -->
            ,student_id = #{studentId}
        </if>
        <if test=" date != null and date !='' "><!-- 日期 -->
            ,date = #{date}
        </if>
        <if test=" startTime != null and startTime !='' "><!-- 开始时间 -->
            ,start_time = #{startTime}
        </if>
        <if test=" endTime != null and endTime !='' "><!-- 结束时间 -->
            ,end_time = #{endTime}
        </if>
        <if test=" deviceId != null and deviceId !='' "><!-- 学校设备id -->
            ,device_id = #{deviceId}
        </if>
        <if test=" flag != null and flag !='' "><!-- 规定行为状态 -->
            ,flag = #{flag}
        </if>
        <if test=" actualFlag != null and actualFlag !='' "><!-- 实际行为状态 -->
            ,actual_flag = #{actualFlag}
        </if>
        <if test=" isAlarm != null and isAlarm !='' "><!-- 是否报警 -->
            ,is_alarm = #{isAlarm}
        </if>
        <if test=" isUse != null and isUse !='' "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null and createTime !='' "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null and creator !='' "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null and updator !='' "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='AlarmDBO'>
        UPDATE alarm SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='AlarmDBO'>
        DELETE FROM alarm WHERE update_time = #{updateTime}
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='AlarmDBO' resultType='AlarmDBO'>
        SELECT <include refid='tableColumns'/> FROM alarm WHERE 1 = 1
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </select>

</mapper>
