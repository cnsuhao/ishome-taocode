    <delete id='toDelete' parameterType='OaExamineResultDBO'>
        UPDATE oa_examine_result SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" id != null and id !='' "><!-- 自增id -->
                AND id = #{id}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
