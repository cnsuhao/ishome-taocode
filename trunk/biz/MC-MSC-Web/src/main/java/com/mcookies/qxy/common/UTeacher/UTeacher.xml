<!-- 教师表 -->
<!-- 需要和DAO保持完全路径一致 -->
<mapper namespace='com.mcookies.qxy.common.UTeacher.UTeacherDao'>
    <!-- 表字段信息  -->
    <sql id='tableColumns'>
        tid,uid,sid,teacher_name,phone,email,id_type,method,openid,wechatname,wechathead,city,gender,status,is_use,create_time,creator,update_time,updator
    </sql>

    <!-- 根据条件更新所有记录 -->
    <update id='doUpdateAll' parameterType='UTeacherDBO'>
        UPDATE u_teacher SET update_time = #{updateTime} <!-- 更新时间、更新者 -->
        <if test=" tid != null "><!-- 教师id（自增） -->
            ,tid = #{tid}
        </if>
        <if test=" uid != null "><!-- 用户id -->
            ,uid = #{uid}
        </if>
        <if test=" sid != null "><!-- 学校id -->
            ,sid = #{sid}
        </if>
        <if test=" teacherName != null "><!-- 教师姓名 -->
            ,teacher_name = #{teacherName}
        </if>
        <if test=" phone != null "><!-- 联系电话 -->
            ,phone = #{phone}
        </if>
        <if test=" email != null "><!-- 邮箱 -->
            ,email = #{email}
        </if>
        <if test=" idType != null "><!-- 用户类型 -->
            ,id_type = #{idType}
        </if>
        <if test=" openid != null "><!-- openid -->
            ,openid = #{openid}
        </if>
        <if test=" wechatname != null "><!-- 微信名（不启用） -->
            ,wechatname = #{wechatname}
        </if>
        <if test=" wechathead != null "><!-- 微信头像 -->
            ,wechathead = #{wechathead}
        </if>
        <if test=" city != null "><!-- 城市 -->
            ,city = #{city}
        </if>
        <if test=" gender != null "><!-- 性别 -->
            ,gender = #{gender}
        </if>
        <if test=" status != null "><!-- 状态 -->
            ,status = #{status}
        </if>
        <if test=" isUse != null "><!-- 是否启用 -->
            ,is_use = #{isUse}
        </if>
        <if test=" createTime != null "><!-- 创建时间 -->
            ,create_time = #{createTime}
        </if>
        <if test=" creator != null "><!-- 创建者 -->
            ,creator = #{creator}
        </if>
        <if test=" updator != null "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" tid != null "><!-- 教师id（自增） -->
                AND tid = #{tid}
            </if>
            <if test=" uid != null "><!-- 用户id -->
                AND uid = #{uid}
            </if>
            <if test=" sid != null "><!-- 学校id -->
                AND sid = #{sid}
            </if>
            <if test=" teacherName != null "><!-- 教师姓名 -->
                AND teacher_name = #{teacherName}
            </if>
            <if test=" phone != null "><!-- 联系电话 -->
                AND phone = #{phone}
            </if>
            <if test=" email != null "><!-- 邮箱 -->
                AND email = #{email}
            </if>
            <if test=" idType != null "><!-- 用户类型 -->
                AND id_type = #{idType}
            </if>
            <if test=" openid != null "><!-- openid -->
                AND openid = #{openid}
            </if>
            <if test=" wechatname != null "><!-- 微信名（不启用） -->
                AND wechatname = #{wechatname}
            </if>
            <if test=" wechathead != null "><!-- 微信头像 -->
                AND wechathead = #{wechathead}
            </if>
            <if test=" city != null "><!-- 城市 -->
                AND city = #{city}
            </if>
            <if test=" gender != null "><!-- 性别 -->
                AND gender = #{gender}
            </if>
            <if test=" status != null "><!-- 状态 -->
                AND status = #{status}
            </if>
            <if test=" isUse != null "><!-- 是否启用 -->
                AND is_use = #{isUse}
            </if>
            <if test=" createTime != null "><!-- 创建时间 -->
                AND create_time = #{createTime}
            </if>
            <if test=" creator != null "><!-- 创建者 -->
                AND creator = #{creator}
            </if>
            <if test=" updateTime != null "><!-- 更新时间 -->
                AND update_time = #{updateTime}
            </if>
            <if test=" updator != null "><!-- 最后更新者 -->
                AND updator = #{updator}
            </if>
    </update>
    <!-- 查询分页数据信息  -->
    <select id='doSelectPage' parameterType='UTeacherDBO' resultType='UTeacherDBO'>
        SELECT <include refid='tableColumns'/> FROM u_teacher
        WHERE 1 = 1 
        <if test=" tid != null "><!-- 教师id（自增） -->
