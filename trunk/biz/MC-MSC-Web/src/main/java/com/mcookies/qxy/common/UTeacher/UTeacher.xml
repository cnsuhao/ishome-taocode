            ,creator = #{creator}
        </if>
        <if test=" updator != null "><!-- 最后更新者 -->
            ,updator = #{updator}
        </if>
        WHERE 1 = 1 
            <if test=" tid != null "><!-- 教师id（自增） -->
                AND tid = #{tid}
            </if>
    </update>
    <!-- 逻辑删除一条数据 -->
    <delete id='toDelete' parameterType='UTeacherDBO'>
        UPDATE u_teacher SET ddd='1', update_time = #{updateTime}, uu2 = #{uu2} WHERE update_time = #{updateTime}
            <if test=" tid != null "><!-- 教师id（自增） -->
                AND tid = #{tid}
            </if>
    </delete>
    <!-- 物理删除一条数据 -->
    <delete id='doDelete' parameterType='UTeacherDBO'>
        DELETE FROM u_teacher WHERE tid = #{tid}
    </delete>
    <!-- 查询一条数据  -->
    <select id='doRead' parameterType='UTeacherDBO' resultType='UTeacherDBO'>
        SELECT <include refid='tableColumns'/> FROM u_teacher WHERE 1 = 1
